# .env.local.example
GITHUB_TOKEN=ghp_your_token_here

# To access environment variables in clientside code, prefix them with NEXT_PUBLIC
# comment this line to input a username in the client
NEXT_PUBLIC_GITHUB_USERNAMES=octocat,hubot,otheruser

# Honeycomb Frontend Observability Configuration
# Get your API key from https://ui.honeycomb.io/account
NEXT_PUBLIC_HONEYCOMB_API_KEY=your_honeycomb_api_key_here
NEXT_PUBLIC_HONEYCOMB_SERVICE_NAME=github-contributions-frontend
NEXT_PUBLIC_ENVIRONMENT=development
